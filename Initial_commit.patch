+ */
+
+public class RefreshableView extends LinearLayout implements View.OnTouchListener {
+    public static final int STATUS_PULL_TO_REFRESH = 0;
+
+    /**
+     * 释放立即刷新状态
+     */
+    public static final int STATUS_RELEASE_TO_REFRESH = 1;
