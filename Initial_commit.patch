+    private ListView listView;
+
+    /**
+     * 刷新时显示的进度条
+     */
+    private ProgressBar progressBar;
+
+    /**
+     * 指示下拉和释放的箭头
+     */
+    private ImageView arrow;
+
+    /**
+     * 指示下拉和释放的文字描述
+     */
+    private TextView description;
+
+    /**
+     * 上次更新时间的文字描述
